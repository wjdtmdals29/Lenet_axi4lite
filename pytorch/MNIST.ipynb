def convert_img_to_txt():
    image_location = "C:\\Users\\newb\\Desktop\\MNIST_test_image\\9\\img"
    image_extension = ".jpg"
    image_data = []
    for i in range(1, 1001):
        img_path = f"{image_location}{i}{image_extension}"
        img = Image.open(img_path, "r")
        np_img = np.array(img)
        np_img_re = np.reshape(np_img,(28,28))
        image_data.append(np_img_re)
        img.close()
    
    stacked_data = np.vstack(image_data)
    np.savetxt('test_num9.txt', stacked_data, fmt='%d',delimiter = " ")
   
    
